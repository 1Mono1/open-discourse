{
  "name": "srv-database",
  "version": "1.0.0",
  "author": "Limebit GmbH <oliver.franke@limebit.de>",
  "license": "MIT",
  "scripts": {
    "db:update:local": "ts-node -r dotenv/config ./src/model/dbUpdate.ts dotenv_config_path=./.env.local",
    "db:dump:schema": "pg_dump -s -O -f schema.sql postgres://postgres:postgres@localhost:5432/next",
    "db:dump:data": "pg_dump --column-inserts --data-only -f data.sql postgres://postgres:postgres@localhost:5432/next",
    "format": "prettier --write \"./**/*.{ts,js,json,md}\" --ignore-path ../.prettierignore",
    "lint": "eslint . --ext .js,.ts --ignore-path='../.eslintignore'",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "@aws-cdk/aws-rds": "1.32.2",
    "@aws-cdk/aws-secretsmanager": "1.32.2",
    "@types/jest": "25.1.4",
    "@types/pg": "7.14.3",
    "db-migrate": "0.11.6",
    "db-migrate-pg": "1.0.0",
    "db-migrate-sqlite3": "0.4.0",
    "dotenv": "8.2.0",
    "faker": "4.1.0",
    "jest": "25.2.3",
    "pg": "7.18.2",
    "ts-jest": "25.2.1",
    "ts-node": "8.8.1",
    "prettier": "^1.19.1",
    "typescript": "3.8.3",
    "eslint": "^6.7.0",
    "aws-sdk": "2.690.0"
  },
  "engines": {
    "node": "^12.0"
  }
}
